{
  "name": "nodetank",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretypeorm": "ts-node -r tsconfig-paths/register ./src/database/scripts/copy-typeorm-config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm -- migration:generate -n migs",
    "migration:create": "yarn typeorm -- migration:create -n migs",
    "migration:run": "yarn typeorm -- migration:run",
    "migration:revert": "yarn typeorm -- migration:revert",
    "schema:drop": "yarn typeorm -- schema:drop",
    "start": "NODE_ENV=local nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "test": "jest",
    "test:watch": "jest --watch",
  },
  "dependencies": {
    "@admin-bro/typeorm": "^1.4.0",
    "@adminjs/express": "^4.1.0",
    "@adminjs/nestjs": "^4.0.1",
    "@adminjs/typeorm": "^2.0.4",
    "@nestjs-modules/mailer": "1.6.1",
    "@nestjs/common": "8.4.3",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.4.3",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.3",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.3",
    "@nestjs/websockets": "^8.4.4",
    "admin-bro": "^4.0.1",
    "adminjs": "^5.9.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "google-auth-library": "^8.0.1",
    "handlebars": "4.7.7",
    "multer": "1.4.4",
    "nestjs-i18n": "8.3.2",
    "nodemailer": "6.7.2",
    "passport": "0.5.2",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "8.7.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "socket.io": "^4.5.0",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "4.3.0",
    "ts-mixer": "^6.0.1",
    "typeorm": "0.2.43",
    "typeorm-seeding": "1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.4",
    "@nestjs/schematics": "8.0.8",
    "@nestjs/testing": "8.4.3",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.25",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.6",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "env-cmd": "10.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "prettier": "2.6.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "yarn": "1.22.18"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "14.19.1",
    "yarn": "1.22.18"
  }
}
